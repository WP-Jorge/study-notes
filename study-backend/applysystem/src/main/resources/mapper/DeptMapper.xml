<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cslg.applysystem.mapper.DeptMapper">
    <cache-ref namespace="cn.cslg.applysystem.mapper.DeptMapper"/>

    <select id="getAllDeptsWithPage" resultType="cn.cslg.applysystem.pojo.vo.DeptVO">
        select * from dept
    </select>

    <select id="getAllDepts" resultType="cn.cslg.applysystem.pojo.vo.DeptVO">
        select * from dept
    </select>

    <insert id="addDept">
        insert into dept (dept_name) values (#{deptName})
    </insert>

</mapper>
