<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cslg.applysystem.mapper.FileMapper">
    <!--<cache-ref namespace="cn.cslg.applysystem.mapper.FileMapper"/>-->
    <cache-ref namespace="cn.cslg.applysystem.mapper.FileTypeMapper"/>
    <update id="updateFIleRefCountByFileIdSub">
        update file set ref_count = ref_count - 1 where file_id = #{fileId}
    </update>
    <update id="updateFIleRefCountByFileIdPlus">
        update file set ref_count = ref_count + 1 where file_id = #{fileId}
    </update>
</mapper>
