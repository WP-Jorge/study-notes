<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cslg.applysystem.mapper.UserRoleMapper">
    <cache-ref namespace="cn.cslg.applysystem.mapper.UserRoleMapper"/>
    <cache-ref namespace="cn.cslg.applysystem.mapper.UserMapper"/>
    <cache-ref namespace="cn.cslg.applysystem.mapper.RoleMapper"/>
    <insert id="addUserRole">
        insert into user_role (user_id, role_id) values(#{userId}, #{roleId})
    </insert>
</mapper>
