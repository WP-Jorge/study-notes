<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.boxmusic.mapper.AlbumMapper">
    <cache-ref namespace="com.example.boxmusic.mapper.MusicMapper"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.boxmusic.pojo.entity.Album">
        <id column="album_id" property="albumId" />
        <result column="album_name" property="albumName" />
        <result column="album_pic" property="albumPic" />
        <result column="album_description" property="albumDescription" />
        <result column="total_views" property="totalViews" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>
    
    <resultMap id="AlbumVOResultMap" type="com.example.boxmusic.pojo.vo.AlbumVO">
        <id column="album_id" property="albumId" />
        <result column="album_name" property="albumName" />
        <result column="album_pic" property="albumPic" />
        <result column="album_description" property="albumDescription" />
        <result column="total_views" property="totalViews" />
    </resultMap>

</mapper>
