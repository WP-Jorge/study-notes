<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team001.mapper.RoomMapper">


    <select id="getRoomList" resultMap="roomMap">
  select m_room.*, m_room_type.room_type_name
  from m_room left join m_room_type on m_room.tno = m_room_type.tno
 </select>


    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.team001.entity.Room" id="roomMap">
        <result property="mno" column="mno"/>
        <result property="tno" column="tno"/>
        <result property="contain" column="contain"/>
        <result property="position" column="position"/>
        <result property="pic" column="pic"/>
        <result property="name" column="name"/>
        <result property="description" column="description"/>
        <result property="enabled" column="enabled"/>
        <result property="createTime" column="create_time"/>
        <result property="device" column="device" />
        <result property="type" column="room_type_name" />
    </resultMap>
</mapper>
